<%- if @exception.blank? %>
  <%- @output.each do |output| %>
    <%= "console.log('#{j output}');".html_safe %>
  <% end %>

  console.log("=> <%= @result %>");
<%- else %>
  console.log("<%= escape_javascript @exception %>");
<% end %>

printError('<%= j output %>');

